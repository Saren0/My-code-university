Program palachnin11;
Var

 A: array [1..100] of Integer;
 N: byte;
 k, i: integer;
 
 begin
 
Write ('Введите размерность массива N=');
ReadLn (N);
WriteLn ('Введите элементы массива');
For i:=1 to N do
    begin
      Write ('A[',i,']=');
      ReadLn (A[i]);
end;

      k:=0;
For i:=1 to N do
   If A[i]=0 then k:=i;
    writeln(k);
end.
